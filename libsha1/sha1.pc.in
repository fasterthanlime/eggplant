prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: @PACKAGE_NAME@
Description: sha1 public domain code packaged for good
Version: @PACKAGE_VERSION@
URL: @PACKAGE_URL@
Libs: -L${libdir} -lsha1-@SHA1_API_VERSION@
Cflags: -I${includedir}/sha1-@SHA1_API_VERSION@ -I${libdir}/sha1-@SHA1_API_VERSION@/include
